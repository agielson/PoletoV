{
  "expo": {
    "name": "my-expo-app",
    "slug": "my-expo-app",
    "version": "1.0.0",
    "scheme": "myapp",
    "web": {
      "favicon": "./assets/images/icon.png"
    },

    "experiments": {
      "tsconfigPaths": true,
      "newArchEnabled": true
    },

    "plugins": [],

    "orientation": "portrait",
    "icon": "./assets/images/icon.png",

    "userInterfaceStyle": "light",

    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    }
  }
}
